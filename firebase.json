{
  "firebase": "volfieddraw",
  "public": ".",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "rewrites": [
    {
      "source": "/drawings",
      "destination": "/viewer.html"
    },
    {
      "source": "/drawings/*",
      "destination": "/viewer.html"
    }
  ]
}